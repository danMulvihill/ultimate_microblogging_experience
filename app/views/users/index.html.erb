<p id="notice"><%= notice %></p>

<h1>User List</h1>
  <ul>
    <% @users.each do |user| %>
      
        <li><%= user.fname %> <%= user.lname %>
          <span>
              <!--    <%= link_to 'Show', user %> -->
              <%= link_to 'Edit', edit_user_path(user) %>
               <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %>
          </span>
          </li>
    <% end %>
    </ul> 
